<table class="centered striped responsive-table">
   <thead>
      <tr>
         <th></th>
         <th>Destination</th>
         <th>Shipper</th>
         <th>Pallet Count</th>
         <th>Piece Count</th>
         <th></th>
      </tr>
   </thead>
   <tbody>
      <% @returns.each_with_index do |r, index| %>
         <tr>
            <td><%= index + 1 %></td>
            <td><%= r.distro_center %></td>
            <td><%= r.shipper %></td>
            <td><%= r.pallet_count %></td>
            <td><%= r.piece_count %></td>
            <td><%= link_to "Generate RGA Label", "#", class: "waves-effect waves-light btn hoverable"  %></td>
         </tr>
      <% end %>
   </tbody>
</table>
